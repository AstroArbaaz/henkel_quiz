<body>
  <div class="henkel_logo">
    <img src="/img/henkel-1-logo-svg-vector.svg" alt="">
  </div>
  <div class="wrapper">

  

    <table class="tabletop text-center justify-content-center" style="width: 100%;">
      <thead class=" align-items-center">
        <tr>
          <th class="table-head " style="width: 20%;">Rank</th>
          <th class="table-head " style="width: 40%">Name</th>
          <th class="table-head ">Score</th>
          <!-- <th class="table-head ">Time Taken</th> -->
        </tr>
      </thead>
      <tbody>

        <% details.forEach(function(Leaderboard, index=1){ %>
          <tr style="font-size: 1.3em; font-weight: 400">
            <td><%= index+1%></td>
            <td><%= Leaderboard.name%></td>
            <td><%= Leaderboard.score%></td>
            <!-- 0<td><%= Leaderboard.time%></td> -->
          </tr>
        <% }) %>

